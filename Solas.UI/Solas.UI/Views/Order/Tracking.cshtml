@using Solas.BL.Models;
@model Solas.BL.Models.Order
@{
    ViewData["Title"] = "Tracking";
}

<!-- about-heading area start  -->
<div class="about-heading">
	<div class="container container-small">
		<div class="row">
			<div class="col-lg-12">
				<div class="section-main-title mb-40">
					<h5 class="section-main-title mb-40">
						Your Tracking Number :<br />" @Model.TrackingId"
					</h5>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- about-heading area end  -->
<!-- features area start  -->
<div class="features-area features-area4">
    <div class="container container-small">
        <div class="row">
			<div class="container">
				<div class="container">
							<hr />
							<hr />
					<div class="col-lg-12">

						<div class="row justify-content-center">

								<h5 class="section-main-title mb-40">
									Order is @Model.Status</h5>
							

						</div>
					</div>
						<hr />
							<hr />

				
				</div>
			</div>
        </div>
    </div>
</div>
<!-- features area end  -->
@await Component.InvokeAsync("Category", new { action = "banner" })

